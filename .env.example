# Polymarket market selection (defaults work for BTC 15-min markets)
# POLYMARKET_SLUG=
# POLYMARKET_SERIES_ID=
# POLYMARKET_SERIES_SLUG=
# POLYMARKET_AUTO_SELECT_LATEST=true

# Polygon RPC (comma-separated for fallback)
# POLYGON_RPC_URL=https://polygon-rpc.com
# POLYGON_RPC_URLS=https://polygon-rpc.com,https://rpc.ankr.com/polygon
# POLYGON_WSS_URL=wss://polygon-bor-rpc.publicnode.com
# POLYGON_WSS_URLS=wss://polygon-bor-rpc.publicnode.com

# Chainlink oracle (defaults to mainnet BTC/USD aggregator)
# CHAINLINK_BTC_USD_AGGREGATOR=

# Proxy (optional)
# HTTPS_PROXY=
# HTTP_PROXY=
# ALL_PROXY=
